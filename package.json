      "工具"
    ],
    "minVersion": "4.14.0",
    "homepage": "https://github.com/LinmingXHL/napcat-plugin-manborvc"
  },
  "dependencies": {
    "napcat-types": "0.0.16"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@types/express": "^5.0.6",
    "@types/node": "^20.19.33",
    "napcat-plugin-debug-cli": "^1.2.8",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "ws": "^8.19.0"
  },
  "scripts": {
